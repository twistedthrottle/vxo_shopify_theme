<div class="social-icons__wrapper">
  {% if settings.facebook_url != blank %}
    <a href="{{ settings.facebook_url }}" target="_blank"  aria-label="Facebook">
      {% render 'snip-icons',
           wrapper: wrapper,
           type: 'social',
           icon: 'facebook',
           classes: 'social-icons--icon',
           size: '14px',
           fill: fill_color,
           hover: hover_color %}
    </a>
  {% endif %}
