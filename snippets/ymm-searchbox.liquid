    }
    
    //Display updated garage contents.
    displayGarage(returnedData);
   
    //Update garage item count.
  	// $m(".ymm-garage-container .ymm_counter").html("("+returnedData.length+")");
    $m(".ymm-garage-container .ymm_counter").html(returnedData.length);
    
    return true;   
  } else {
    return false;
  }
}

